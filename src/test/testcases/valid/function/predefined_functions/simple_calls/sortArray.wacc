# Testing sorting

# Output:
# [1, 1, 2, 2, 3, 4, 4, 6, 6, 7, 8, 8, 56, 56]
# [9, 8, 7, 5, 5, 4, 2, 1, 1]

# Program:

import WACCarray

begin 

  int[] arr = [1,2,56,7,56,1,2,6,8,4,3,8,4,6];
  arr = call sort(arr, false);
  arr = call printArray(arr);

  int[] arr1 = [1,5,4,7,8,9,1,2,5];
  arr1 = call sort(arr1, true);
  arr1 = call printArray(arr1)

end
